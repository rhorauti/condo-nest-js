generator mongoClient {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/mongo-client"
}

datasource mongodb {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

model Message {
  id      String @id @default(auto()) @map("_id") @mongodb.ObjectId
  content String
  // ...
   @@map("user")
}
